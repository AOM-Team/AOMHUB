local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Rayfield/main/source'))()

local Window = Rayfield:CreateWindow({
   Name = "étapes de vérification du AOM hub",
   LoadingTitle = "AOM Hub V2",
   LoadingSubtitle = "Par Marco Olivier et Aie ",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil, -- Create a custom folder for your hub/game
      FileName = "AOM Hub"
   },
   Discord = {
      Enabled = false,
      Invite = "AOMForce", -- The Discord invite code, do not include discord.gg/. E.g. discord.gg/ABCD would be ABCD.
      RememberJoins = true -- Set this to false to make them join the discord every time they load it up
   },
   KeySystem = true, -- Set this to true to use our key system
   KeySettings = {
      Title = "AOM Hub V2",
      Subtitle = "Système de sécurité par clé",
      Note = "Merci à l'équipe AOM pour le projet",
      FileName = "AOM Key",
      SaveKey = true,
      GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
      Key = "MarcoOlivierAmbre"
   }
})

local Tab = Window:CreateTab("Accès au hub", 4483362458) -- Title, Image

local Section = Tab:CreateSection("Hub")

local Button = Tab:CreateButton({
   Name = "un test de merde de niaiseur",
   Callback = function()
   print("Hello World!")
   end,
})

